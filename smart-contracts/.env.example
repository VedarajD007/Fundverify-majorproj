# Smart Contracts Environment Variables
# Copy this file to .env and fill in your keys

# Sepolia RPC URL (get from Alchemy/Infura)
# Get from: https://www.alchemy.com/
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your_alchemy_api_key_here

# Private key for deployment (without 0x prefix)
# Use a dedicated test wallet only!
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification (optional)
# Get from: https://etherscan.io/apis
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas settings
GAS_LIMIT=6000000
GAS_PRICE=20000000000
