# Frontend Environment Variables
# Copy this file to .env.local and update the values

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:8000

# Smart Contract Addresses (will be filled after deployment)
NEXT_PUBLIC_FUND_MANAGEMENT_CONTRACT=your_deployed_contract_address
NEXT_PUBLIC_SBT_CONTRACT=your_deployed_sbt_contract_address

# Blockchain Network
NEXT_PUBLIC_NETWORK=sepolia
NEXT_PUBLIC_CHAIN_ID=11155111

# Web3 RPC URL (get from Alchemy)
NEXT_PUBLIC_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your_alchemy_api_key_here

# IPFS settings
NEXT_PUBLIC_IPFS_GATEWAY=https://ipfs.io/ipfs/
